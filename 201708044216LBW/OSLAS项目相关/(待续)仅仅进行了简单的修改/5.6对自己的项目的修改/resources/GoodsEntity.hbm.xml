<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.zut.cs.software.dao.GoodsEntity" table="goods" schema="goods">
        <id name="goodId" type="java.lang.String">
            <column name="good_id"></column>
            <generator class="native"></generator>
        </id>
        <property name="goodName" type="java.lang.String">
            <column name="good_name"></column>
        </property>
        <property name="number" type="java.lang.String">
            <column name="number"></column>
        </property>
    </class>
</hibernate-mapping>