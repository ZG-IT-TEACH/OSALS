<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.zut.cs.software.daomain.GoodKind" table="goodkinds" schema="goods">

        <id name="kindNames" type="java.lang.String" >
            <column name="kind_name"></column>
        </id>
        <property name="goods" type="java.io.Serializable">
        </property>
    </class>
</hibernate-mapping>