<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.zut.cs.software.daomain.Good" table="goods" schema="goods">
        <id name="GoodId" column="good_id" type="java.lang.String"></id>
        <property name="goodNames" type="java.lang.String">
            <column name="good_name"></column>
        </property>
        <property name="numbers" type="java.lang.Integer">
            <column name="number"></column>
        </property>

    </class>
</hibernate-mapping>